<Window x:Class="HydacProjekt.Views.ReportScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HydacProjekt.Views"
        mc:Ignorable="d"
        Title="ReportScreen" Height="450" Width="800">
    <Grid>
        <Rectangle HorizontalAlignment="Center" Height="60" VerticalAlignment="Top" Width="800" Fill="DimGray"/>
        <Image Margin="0,-7,556,367" Source="/HYDAC_Logo_NOVO_(1).png" Stretch="Fill"/>
        <Image Margin="216,-16,216,313" Source="/Images/HYDAC_Logo_NOVO_(1).png" Stretch="Fill"/>
        <ComboBox x:Name="ChooseProduct" HorizontalAlignment="Left" Margin="232,121,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Center" Margin="0,174,0,0" TextWrapping="Wrap" Text="Report" VerticalAlignment="Top" Width="336" Height="182"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="232,361,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="539,361,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="232,148,0,0" TextWrapping="Wrap" Text="Report Title" VerticalAlignment="Top" Width="168" Height="21"/>

    </Grid>
</Window>
